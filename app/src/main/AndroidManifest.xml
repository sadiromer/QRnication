<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.qrnication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Scan.ScanActivity" />
        <activity android:name=".Generate.GenerateActivity" />
        <activity android:name=".Scan.ScanSingleActivity" />
        <activity android:name=".Scan.ScanContinuousActivity" />
        <activity android:name=".Generate.GenerateTextActivity" />
        <activity android:name=".Generate.GenerateImageActivity"></activity>
    </application>

</manifest>